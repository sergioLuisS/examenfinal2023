<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="edu.umg.DTO.UsuariosDTO" table="usuarios" schema="public" catalog="dbProyectoFinal">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="nombreUsuario">
            <column name="nombre_usuario" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="contrasena">
            <column name="contrasena" sql-type="varchar(100)" length="100"/>
        </property>
    </class>
</hibernate-mapping>